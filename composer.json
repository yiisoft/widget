{
    "name": "yiisoft/widget",
    "type": "library",
    "description": "Widget",
    "keywords": [
        "yii",
        "widgets"
    ],
    "homepage": "https://www.yiiframework.com/",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/yiisoft/widget/issues?state=open",
        "forum": "https://www.yiiframework.com/forum/",
        "wiki": "https://www.yiiframework.com/wiki/",
        "irc": "irc://irc.freenode.net/yii",
        "source": "https://github.com/yiisoft/widget"
    },
    "minimum-stability": "dev",
    "require": {
        "php": "^7.4|^8.0",
        "psr/container": "1.0.0",
        "yiisoft/di": "^3.0@dev",
        "yiisoft/factory": "^3.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "phan/phan": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Yiisoft\\Widget\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Yiisoft\\Widget\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        },
        "config-plugin": {
            "providers": "config/providers.php"
        }
    },
    "scripts": {
        "phan": "phan --progress-bar -o analysis.txt",
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "prefer-stable": true
}
